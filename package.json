    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.12.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-livereload-plugin": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^2.10.2",
    "body-parser": "^1.13.3",
    "bootstrap": "3.3.5",
    "d3": "^4.12.2",
    "express": "^4.13.3",
    "fs": "0.0.1-security",
    "jsonfile": "^4.0.0",
    "mapbox-gl": "^0.40.1",
